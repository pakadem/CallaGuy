<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.call_a_guy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/callaguylogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/callaguylogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.CallaGuy">
        <activity android:name=".ClientViewActivity"></activity>
        <activity android:name=".ClientRegisterActivity" />
        <activity android:name=".ClientStartActivity" />
        <activity android:name=".ArtisanViewActivity" />
        <activity android:name=".ArtisanRegisterActivity" />
        <activity android:name=".ArtisanStartActivity" />
        <activity android:name=".ArtisanActivity" />
        <activity android:name=".Splash" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>